{
    "name": "cundd/fleet",
    "type": "typo3-cms-extension",
    "description": "Command line utility to provide system information for fleet",
    "keywords": [
        "typo3cms",
        "fleet",
        "service"
    ],
    "require": {
        "php": "~5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.4"
    },
    "homepage": "https://fleet.cundd.net/",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cundd\\Fleet\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\Fleet\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "fleet": "self.version",
        "typo3-ter/fleet": "self.version"
    }
}
